{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5"
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  }
}
